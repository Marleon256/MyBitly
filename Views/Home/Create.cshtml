@model MyBitly.Models.UrlData
@{
    ViewBag.Title = "Add url";
}
<form asp-action="create" asp-controller="home">
    <div class="form-group">
        <label asp-for="FullUrl" class="control-label">Full url</label>
        <input type="text" asp-for="FullUrl" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Send" class="btn btn-default" />
    </div>
</form>