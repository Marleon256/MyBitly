@model MyBitly.Models.UrlData
@{
    ViewBag.Title = "Edit url";
}
<form asp-action="Edit" asp-controller="Home" asp-route-shortUrl="@Model.ShortUrl">
    <div class="form-group">
        <label asp-for="FullUrl" class="control-label">New full url</label>
        <input type="text" asp-for="FullUrl" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Send" class="btn btn-default" />
    </div>
</form>

